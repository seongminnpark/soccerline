{
  "manifest_version": 2,

  "name": "Soccerline",
  "description": "Make Soccerline Great Again",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["http://soccerline.kr/*"],
      "js": ["scripts/constants.js", "scripts/content-script.js"],
      "run_at": "document_end"
    }
  ],

  "icons": { "16": "icon/icon16.png",
           "48": "icon/icon48.png",
          "128": "icon/icon128.png" },

  "manifest_version": 2
}